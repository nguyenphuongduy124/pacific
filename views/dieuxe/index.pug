- var xeList = Object.keys(data);
html
    head
        link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM' crossorigin='anonymous')
        script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js' integrity='sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz' crossorigin='anonymous')
        style
             include ./style.css
    body
        div.container
            table.table.table-condensed 
                thead 
                    tr 
                        th STT
                        th Xe 
                        th Đại Lý
                        th Số lần vận chuyển
                        th Số lượng
                tbody
                    <!-- data[xe] -->
                    each xe, index in xeList  
                        - var flag = true;
                        - var daiLyList = Object.keys(data[xe]);
                        each daiLy in daiLyList
                            tr(class=flag ? 'firstDaiLy' : '')
                                if flag
                                    td(rowspan=daiLyList.length, class="middle") #{index + 1}
                                    td(rowspan=daiLyList.length, class="middle") #{xe}
                                    #{flag= false}

                                td.info-dai-ly
                                    span.ten-dai-ly #{daiLy}
                                    span.dia-chi (#{data[xe][daiLy]['diaChi']})
                                td #{data[xe][daiLy]['soLanVanChuyen']}
                                td #{data[xe][daiLy]['tongSoLuongBinh']}